{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug android install command",
            "type": "dart",
            "request": "launch",
            "program": "bin/realm.dart",
            "cwd": "${workspaceFolder}/flutter/realm_flutter",
            "args": ["install", "--debug", "--target-os-type", "android", "--package-name", "realm"]
        },
        {
            "name": "Debug ios install command",
            "type": "dart",
            "request": "launch",
            "program": "bin/realm.dart",
            "cwd": "${workspaceFolder}/flutter/realm_flutter",
            "args": ["install", "--debug", "--target-os-type", "ios", "--package-name", "realm"]
        },
        {
            "name": "Debug macos flutter install command",
            "type": "dart",
            "request": "launch",
            "program": "bin/realm.dart",
            "cwd": "${workspaceFolder}/flutter/realm_flutter",
            "args": ["install", "--debug", "--target-os-type", "macos", "--package-name", "realm"]
        },
        {
            "name": "Debug windows install command",
            "type": "dart",
            "request": "launch",
            "program": "bin\\realm_dart.dart",
            "cwd": "${workspaceFolder}",
            "args": ["install", "realm_dart", "--debug"]
        },
        {
            "name": "Flutter: Attach to Device",
            "type": "dart",
            "request": "attach"
        },
        {
            "name": "Flutter: Windows",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "cwd": "${workspaceFolder}/flutter/realm_flutter/example",
            "args": ["-d", "windows"]
        },
        {
            "name": "Flutter: MacOS",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "cwd": "${workspaceFolder}/flutter/realm_flutter/example",
            "args": ["-d", "macos"]
        },
        {
            "name": "(Windows) Attach",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "Dart: Attach to Process",
            "type": "dart",
            "request": "attach",
            "observatoryUri": "http://127.0.0.1:8181/ESrVZJKyfaQ=/",
            "packages": "package:realm/main.dart"
        },
        {
            "name": "Dart: Debug",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Dart: Debug Test",
            "type": "dart",
            "request": "launch",
            "program": "test/realm_test.dart",
            "cwd": "${workspaceFolder}",
            "args": ["--name", ""]
        }
    ]
}